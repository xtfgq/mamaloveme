<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backed"
    android:orientation="vertical" 
    android:clipToPadding="true"
    android:fitsSystemWindows="true">

    <include layout="@layout/title_bar" />
     <com.netlab.loveofmum.widget.ScrollViewExtend 
        
        android:layout_width="fill_parent"  android:layout_height="fill_parent" android:id="@id/sv_index" >
    <LinearLayout 
        android:layout_width="fill_parent"  android:layout_height="fill_parent" android:orientation="vertical"
            >

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <LinearLayout
        android:id="@+id/linear001_userphoto"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@color/white" >
	     <ImageView 
	    android:layout_width="match_parent"
	    android:layout_height="1dp"
	    android:background="#EDEDED"
	    />   
	    <LinearLayout 
	    android:layout_width="match_parent"
	 	android:layout_marginLeft="15dip"
	    android:layout_marginRight="15dip"
	    android:layout_height="50dp"
	    android:orientation="horizontal"
	    android:gravity="center_vertical"
	    android:weightSum="10"
	    >
	    <ImageView
	       style="@style/global_wwhw"
	       android:layout_width="25dip"
	       android:src="@drawable/icon_prot"
	      
	        />
	    <TextView 
	        
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_marginLeft="5dip"
	        android:textSize="15sp"
	        android:gravity="center_vertical"
	        android:maxLength="10"
	        android:background="@color/white"
	        android:text="头像修改"
	        android:layout_weight="10"
	        /> 
		<com.netlab.loveofmum.widget.CircularImage
	       android:layout_height="45dp"
	       android:id="@+id/img_userphoto"
	       android:layout_width="45dp"
	        />
	</LinearLayout>
	</LinearLayout>

    <include layout="@layout/title_bar10" />

   
    
    <include layout="@layout/title_bar11" />
		<View
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:background="#EDEDED"
			/>
		 <LinearLayout
			 android:layout_width="match_parent"
			 android:layout_height="wrap_content"
			 android:orientation="vertical"
			 android:background="@color/white" >
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_marginLeft="15dip"
			android:layout_marginRight="15dip"
			android:layout_height="50dip"
			android:background="@color/white"
			android:orientation="horizontal"
			android:gravity="center_vertical"
			>
			<ImageView
				style="@style/global_wwhw"
				android:layout_width="25dip"

				android:src="@drawable/str_ordercardno"
				/>
			<EditText
				android:id="@+id/id_title_jiuzhen"
				android:layout_marginLeft="5dip"
				android:textSize="15sp"
				android:maxLength="20"
				android:inputType="number"
				android:digits="0123456789abcdefghigklmnopqrstuvwxyzABCDEFGHIGKLMNOPQRSTUVWXYZ"
				style="@style/global_wwwww"
				android:background="@color/white"
				android:hint="@string/str_ordercardno"
				/>

		</LinearLayout>

		 </LinearLayout>
    <include layout="@layout/title_bar12" />

    <LinearLayout 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="vertical"
		    android:background="@color/white" >
		     <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="#EDEDED"
		    />  <LinearLayout 
		    android:layout_width="match_parent"
		     android:layout_marginLeft="15dip"
		        android:layout_marginRight="15dip"
		    android:layout_height="50dip"
		    android:orientation="horizontal"
		    android:gravity="center_vertical"
		    >
		    <ImageView
		       style="@style/global_wwhw"
		          android:layout_width="25dip"
		       android:src="@drawable/icon_nl"
		        />

		    <EditText
		        android:id="@+id/txt01_userinfochange"
		        style="@style/global_wwwww"
		        android:layout_marginLeft="5dip"
		        android:background="@color/white"
		        android:hint="@string/reg_age"
		        android:inputType="number"
		        android:maxLength="20"
		        android:textSize="15sp" />
		
		</LinearLayout>
		</LinearLayout>
    
    <include layout="@layout/title_bar14" />
 <LinearLayout 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="vertical"
		    android:background="@color/white" >
		     <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="#EDEDED"
		    />   
		    <LinearLayout 
		    android:layout_width="match_parent"
		 android:layout_marginLeft="15dip"
		        android:layout_marginRight="15dip"
		    android:layout_height="50dip"
		    android:orientation="horizontal"
		    android:gravity="center_vertical"
		    >
		    <ImageView
		       style="@style/global_wwhw"
		       android:layout_width="25dip"
		       android:src="@drawable/icon_nc"
		        />
		    <EditText 
		        android:id="@+id/edt0001_userinfochange"
		        android:layout_marginLeft="5dip"
		        android:textSize="15sp"
		        style="@style/global_wwwww"
		        android:background="@color/white"
		        android:maxLength="20"
		        android:hint="昵称"
		        /> 
		
		</LinearLayout>
		</LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <include layout="@layout/title_bar15" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />
    <RelativeLayout
        android:id="@+id/llyuchan"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/ivdata3"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dip"
               android:layout_width="25dp"
            android:layout_height="25dp"
            
            android:layout_centerVertical="true"
          
            
            
            android:background="@drawable/icon_data3" />

        <TextView

            android:layout_toRightOf="@+id/ivdata3"
            android:id="@+id/txt0001_user_set"
            android:layout_marginLeft="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
           android:layout_centerVertical="true"
            android:text="预产期：" />

      
    </RelativeLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="1dp" />
      <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal" 
        android:background="@color/white"
        >

        <ImageView
            android:id="@+id/ivcard"
            android:layout_width="25dp"
            android:layout_height="25dp"
          
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp"
         android:layout_centerVertical="true"
            android:background="@drawable/icon_card" />
    
        <com.netlab.loveofmum.widget.CustomerSpinner
        android:id="@+id/txthosselect_user_set"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
     android:layout_toRightOf="@+id/ivcard"
    android:textSize="15sp"
      android:layout_centerVertical="true"
			android:spinnerMode="dialog"
        android:background="@null"
      
        >
    </com.netlab.loveofmum.widget.CustomerSpinner>

            
        
    </RelativeLayout>

    <Button
        android:id="@id/btn01_userinfocation"
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/home_btn_bg4"
        android:text="@string/str_genggai"
        android:textColor="@color/white" />
</LinearLayout>
</com.netlab.loveofmum.widget.ScrollViewExtend>
</LinearLayout>