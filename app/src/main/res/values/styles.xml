        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    
    
    
<style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item><!--边框-->
         <item name="android:backgroundDimAmount">0.9</item>
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item><!--无标题-->
       <!-- <item name="android:windowBackground">@color/alpha_bg</item>背景透明-->
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->
           </style>
    <style name="Text" parent="android:style/Widget.TextView">
        <item name="android:shadowColor">#FF000000</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.5</item>
    </style>
 <!-- 全屏activity loading -->
    <style name="loadingDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!-- Timeset -->
    <style name="time_set_dip">
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/timesetcolor002</item>
